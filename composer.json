  "type": "symfony-bundle",
  "license": "MIT",
  "version": "1.0.0",
  "authors": [
    {
      "name": "Andrii Yakovlev",
      "email": "yawa20@gmail.com"
    },
    {
      "name": "Victor Pavlov",
